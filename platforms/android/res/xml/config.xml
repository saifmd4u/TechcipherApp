<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.techcipher" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="splashscreen" value="splash" />
    <preference name="splashScreenDelay" value="10000" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>
    <name>TechCipher</name>
    <description>
      TechCipher is all about exploring technologies available to date.
    </description>
    <author email="team@techcipher.com" href="http://www.techcipher.com">
        TechCipher Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
